{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Show Status Output",
  "type": "object",
  "properties": {
    "current_alias": {
      "type": "string",
      "description": "当前使用的 alias"
    },
    "config": {
      "type": "object",
      "properties": {
        "endpoint": {
          "type": "string"
        },
        "opus_model": {
          "type": "string"
        },
        "sonnet_model": {
          "type": "string"
        },
        "haiku_model": {
          "type": "string"
        },
        "auth_method": {
          "type": "string"
        }
      },
      "description": "当前配置详情"
    },
    "available_aliases": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        }
      },
      "description": "可用的 alias 列表"
    }
  }
}
