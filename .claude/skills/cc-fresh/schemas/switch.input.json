{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Switch Alias Input",
  "type": "object",
  "properties": {
    "alias": {
      "type": "string",
      "enum": ["glm", "claude", "cc"],
      "description": "要切换到的 alias"
    },
    "model": {
      "type": "string",
      "enum": ["opus", "sonnet", "haiku", "default"],
      "default": "default",
      "description": "指定使用的模型级别"
    }
  },
  "required": ["alias"]
}
