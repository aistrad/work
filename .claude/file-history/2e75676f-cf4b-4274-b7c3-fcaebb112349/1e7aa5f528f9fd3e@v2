# å·²æœ‰çš„ SOP State æœºåˆ¶éå¸¸é€‚åˆ Protocol å¼•æ“
# åªéœ€æ‰©å±•ï¼š

async def save_protocol_state(user_id: str, protocol_id: str, state: Dict):
    """ä¿å­˜åè®®è¿›åº¦"""
    await UnifiedProfileRepository.update_skill_state(
        user_id,
        "lifecoach",
        f"protocol:{protocol_id}",
        state
    )

async def resume_protocol(user_id: str, protocol_id: str) -> Optional[Dict]:
    """æ¢å¤æœªå®Œæˆçš„åè®®"""
    state = await UnifiedProfileRepository.get_skill_state(
        user_id,
        "lifecoach",
        f"protocol:{protocol_id}"
    )

    if state and state.get("status") == "in_progress":
        return state  # { current_step, answers, started_at }

    return None
```

