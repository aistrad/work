```python
"""
Companion ä¸»åŠ¨å…³æ€€å¼•æ“
"""
class CompanionEngine:
    """ä¸»åŠ¨å…³æ€€å¼•æ“ï¼ŒåŸºäºç”¨æˆ·çŠ¶æ€ç”Ÿæˆå…³æ€€å¯¹è¯"""

    async def generate_check_in(self, user_id: str) -> Optional[Dict]:
        """ç”Ÿæˆç­¾åˆ°å…³æ€€"""
        profile = await UnifiedProfileRepository.get_profile(user_id)

        # æ£€æŸ¥æ˜¯å¦æœ‰æœªå®Œæˆçš„ç›®æ ‡
        goals = profile.get("life_context", {}).get("goals", [])
        active_goals = [g for g in goals if g.get("status") == "active"]

        if active_goals:
            # é€‰æ‹©æœ€é‡è¦çš„ç›®æ ‡
            goal = max(active_goals, key=lambda g: g.get("priority", 0))

            # ç”Ÿæˆå…³æ€€æ¶ˆæ¯
            return {
                "type": "goal_check_in",
                "title": f"å…³äºã€Œ{goal['title']}ã€",
                "content": await self._generate_check_in_prompt(user_id, goal),
                "suggested_reply": "åˆ†äº«ä¸€ä¸‹è¿›å±•ï¼Ÿ",
            }

        # æ£€æŸ¥æ˜¯å¦æœ‰åå¤å‡ºç°çš„æ¨¡å¼
        themes = profile.get("life_journey", {}).get("recurring_themes", [])
        if themes:
            recent_theme = themes[-1]
            return {
                "type": "pattern_observation",
                "content": f"æˆ‘æ³¨æ„åˆ°ã€Œ{recent_theme['pattern']}ã€è¿™ä¸ªæ¨¡å¼åˆå‡ºç°äº†...",
            }

        return None

    async def _generate_check_in_prompt(self, user_id: str, goal: Dict) -> str:
        """ä½¿ç”¨ LLM ç”Ÿæˆä¸ªæ€§åŒ–çš„å…³æ€€æç¤ºè¯"""
        # è·å–ç”¨æˆ·çš„ preferred master
        preferred_master = "yangming"  # ä» profile è¯»å–

        # è°ƒç”¨ LLM ç”Ÿæˆ
        prompt = f"""
ä½ æ˜¯{preferred_master}ã€‚ç”¨æˆ·ä¸Šæ¬¡å’Œä½ å®šäº†ä¸€ä¸ªç›®æ ‡ï¼š

{goal}

ç°åœ¨è¿‡å»äº†{self._days_since(goal['created_at'])}å¤©ã€‚
è¯·ç”Ÿæˆä¸€å¥ç®€çŸ­çš„å…³æ€€è¯è¯­ï¼Œè¯¢é—®è¿›å±•ã€‚

è¦æ±‚ï¼š
- ç¬¦åˆ{preferred_master}çš„è¯­æ°”
- æ¸©æš–ä½†ä¸æ–½å‹
- å¼•å¯¼ç”¨æˆ·åˆ†äº«
"""
        # ... LLM è°ƒç”¨
```

