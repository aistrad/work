```typescript
function generateGreetingMessage(context: {
  hour: number;
  streak: number;
  checkedIn: boolean;
  weekProgress: number;
  dayOfWeek: number;
}): string {
  const { hour, streak, checkedIn, weekProgress, dayOfWeek } = context;

  // 1. ç‰¹æ®ŠçŠ¶æ€ä¼˜å…ˆ
  if (!checkedIn && hour >= 8 && hour < 12) {
    return "æ–°çš„ä¸€å¤©å¼€å§‹äº†ï¼Œå‡†å¤‡å¥½è¡ŒåŠ¨äº†å—ï¼Ÿ";
  }

  if (streak >= 7) {
    return `è¿ç»­ ${streak} å¤©çš„åšæŒï¼Œä½ æ­£åœ¨åˆ›é€ æ”¹å˜ã€‚`;
  }

  if (dayOfWeek === 1) {
    return "æ–°çš„ä¸€å‘¨ï¼Œæ–°çš„å¯èƒ½ã€‚";
  }

  if (dayOfWeek === 0 || dayOfWeek === 6) {
    return "å‘¨æœ«ä¹Ÿæ˜¯æˆé•¿çš„æ—¶æœºã€‚";
  }

  if (weekProgress >= 0.8) {
    return "è¿™å‘¨å®Œæˆå¾—å¾ˆå¥½ï¼Œç»§ç»­ä¿æŒï¼";
  }

  // 2. æ—¶æ®µé€šç”¨æ¶ˆæ¯
  if (hour < 12) {
    return "ä»Šå¤©æœ€é‡è¦çš„ä¸€ä»¶äº‹æ˜¯ä»€ä¹ˆï¼Ÿ";
  } else if (hour < 18) {
    return "ä¸‹åˆçš„æ—¶å…‰ï¼Œé€‚åˆä¸“æ³¨ã€‚";
  } else {
    return "ä¸€å¤©ç»“æŸï¼Œä¸ºä»Šå¤©çš„åŠªåŠ›æ„Ÿåˆ°éª„å‚²ã€‚";
  }
}
```

